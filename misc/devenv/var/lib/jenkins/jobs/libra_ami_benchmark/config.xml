<?xml version='1.0' encoding='UTF-8'?>
<project>
  <actions/>
  <description>Run the benchmark tasks on the most recent dev or stg AMI.  This set of tests can take from 20 to 30 hours to complete.</description>
  <logRotator>
    <daysToKeep>10</daysToKeep>
    <numToKeep>10</numToKeep>
    <artifactDaysToKeep>10</artifactDaysToKeep>
    <artifactNumToKeep>10</artifactNumToKeep>
  </logRotator>
  <keepDependencies>false</keepDependencies>
  <properties>
    <hudson.model.ParametersDefinitionProperty>
      <parameterDefinitions>
        <hudson.model.StringParameterDefinition>
          <name>use_stage_image</name>
          <description></description>
          <defaultValue>false</defaultValue>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>profile_broker</name>
          <description></description>
          <defaultValue>false</defaultValue>
        </hudson.model.StringParameterDefinition>
      </parameterDefinitions>
    </hudson.model.ParametersDefinitionProperty>
  </properties>
  <scm class="hudson.scm.NullSCM"/>
  <canRoam>true</canRoam>
  <disabled>true</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers class="vector"/>
  <concurrentBuild>false</concurrentBuild>
  <customWorkspace>jobs/libra_ami/workspace</customWorkspace>
  <builders>
    <hudson.tasks.Shell>
      <command>launch_params=&quot;&quot;
if [ &quot;$use_stage_image&quot; == &quot;true&quot; ]
then
launch_params=$launch_params&quot; --use_stage_image&quot;
fi
echo $launch_params

# Launch a test instance
build/devenv launch devenv_benchmark_${BUILD_NUMBER} --verifier $launch_params

test_params=&quot;&quot;
if [ &quot;$profile_broker&quot; == &quot;true&quot; ]
then
test_params=$test_params&quot; --profile-broker&quot;
fi
echo $test_params

# Run the image tests
build/devenv test devenv_benchmark_${BUILD_NUMBER} --exclude_internal --exclude_verification --include-cucumber=benchmark --disable-charlie --terminate $test_params
</command>
    </hudson.tasks.Shell>
  </builders>
  <publishers>
    <hudson.tasks.ArtifactArchiver>
      <artifacts>rhc/log/*</artifacts>
      <latestOnly>false</latestOnly>
    </hudson.tasks.ArtifactArchiver>
  </publishers>
  <buildWrappers/>
</project>
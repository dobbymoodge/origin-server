{
  "proxy_host": null,
  "proxy_port": null,
  "jenkins_host": "https://ci.dev.openshift.redhat.com",
  "jenkins_url": "https://ci.dev.openshift.redhat.com/jenkins/",
  "hub_config": "~/.config/hub",
  "github_user": "openshift",
  "irc_channel": "#openshift-dev",
  "bot_github_user": "openshift-bot",
  
  "repo_groups": {
    "li": "hosted",
    "origin-server": "hosted",
    "rhc": "hosted",
    "origin-dev-tools": "hosted"
  },

  "settings": {
    "test_settings": {
      "name": "test",
      "test_prefix": "Test Results:",
      "allow_multiple": true,
      "branches": { "*": {"jenkins_job_name": "test_pull_requests_generic",
                          "downstream_job_name": "create_ami",
                          "build_token": "libra1",
                          "addtl_jenkins_params": {"hosted": {"DEV_TOOLS_EXT_REPO": "li",
                                                              "DEV_TOOLS_REPO": "origin-dev-tools",
                                                              "RHC_REPO": "rhc",
                                                              "ORIGIN_SERVER_REPO": "origin-server"} }
                             },
                    "master": {"jenkins_job_name": "test_pull_requests",
                               "downstream_job_name": "libra_ami",
                               "build_token": "libra1"} },
      "repo_to_teams": {
        "li": ["168397", "175254", "202863"],
        "origin-server": ["168397", "175254"],
        "rhc": ["168397", "77593"],
        "origin-dev-tools": ["168397", "77593", "175254", "202863"]
      },
      "repo_to_pull_id_param": {
        "li": "DEV_TOOLS_EXT_PULL_ID",
        "origin-server": "ORIGIN_SERVER_PULL_ID",
        "rhc": "RHC_PULL_ID",
        "origin-dev-tools": "DEV_TOOLS_PULL_ID"
      }
    },
    "merge_test_settings": {
      "name": "merge",
      "test_prefix": "Merge Test Results:",
      "allow_multiple": false,
      "branches": { "*": {"jenkins_job_name": "merge_pull_requests_generic",
                              "downstream_job_name": "create_ami",
                              "image_base_name": "devenv-*",
                              "build_token": "libra1",
                              "addtl_jenkins_params": {"hosted": {"DEV_TOOLS_EXT_REPO": "li",
                                                                  "DEV_TOOLS_REPO": "origin-dev-tools",
                                                                  "RHC_REPO": "rhc",
                                                                  "ORIGIN_SERVER_REPO": "origin-server"} }
                             },
                    "master": {"jenkins_job_name": "merge_pull_requests",
                               "downstream_job_name": "libra_ami",
                               "image_base_name": "devenv",
                               "build_token": "libra1"} },
      "repo_to_teams": {
        "li": ["175254", "202863"],
        "origin-server": ["175254"],
        "rhc": ["77593"],
        "origin-dev-tools": ["77593", "175254", "202863"]
      },
      "repo_to_pull_id_param": {
        "li": "DEV_TOOLS_EXT_PULL_ID",
        "origin-server": "ORIGIN_SERVER_PULL_ID",
        "rhc": "RHC_PULL_ID",
        "origin-dev-tools": "DEV_TOOLS_PULL_ID"
      }
    }
  }
}
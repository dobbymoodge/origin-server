}

#slugs li {
        margin-left: 10px;
}

.video_promo img, {
  float: right;
  margin-left: 12px;
  margin-right: 12px;
}

#page h2 {
  font-size: 20pt;
}

#flex_steps h6 {
  font-weight: normal;
}

#flex_steps h6 strong {
  text-transform: uppercase;
}

#flex_steps p {
  font-size: 11pt;
  margin-bottom: 0px;
}

#flex_steps a,
#flex_nowwhat a,
{
  font-size: 11pt;
  display: block;
  text-decoration: none;
  margin-bottom: 1em;
  text-align: right;
  font-weight: bold;
  color: #3C6EB4;
}

#sidebar div {
  margin-left: 10px;
}

#sidebar a.readmore {
  display: block;
  text-decoration: none;
  color: #3C6EB4;
  font-weight: bold;
  text-align: right;

  color: #111;
}

#sidebar ul {
  margin-bottom: 1em;
}

#sidebar img.video_promo {

}

#sidebar {
	background: #e6e6e6;
}

#sidebar div.grid_6 {
	border-bottom: 1px solid #ccc;
	border-top: 1px solid white;
	margin-top: 0px;
	margin-bottom: 0px;
}

#sidebar div.omega { border-bottom: none; }
#sidebar div.alpha { border-top: none; margin-left: 5px; }

#app_promos {
	background: #ececec;
}

#app_promos > div {
	background: #d0d1d0;
}

#footer {
	background: black;
	padding-top: 1.5em;
	padding-bottom: 1em;
}

#subfooter {
	padding-top: 2em;

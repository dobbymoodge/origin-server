	line-height: 16pt;
}

#slugs {
	background: #ececec;
	margin-left: 0px;
	padding-left: 5px;
	padding-bottom: 1em;
	padding-top: 12px;
}

#slugs .suffix_1 {
  padding-left: 15px;
  margin-right: -15px;
}

.slug {
	margin-right: 0px;
	margin-left: 0px;
	padding-left: 5px;
}

.slug a.learn {
	float: right;
	margin-right: 12px;
}

.slug a {
	color: #3771c8;
}

#slugs ul {
        width: 100%;

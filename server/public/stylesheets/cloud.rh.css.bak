body {
    background:#444;
}


#header_right {
	text-align: right;
	position: relative;
}

/* Main nav */

#nav {
	background: url(/app/images/bg_mainnav.png) repeat-x red;
	min-height: 20px;
	padding-top: 7px;
	padding-bottom: 7px;
	border-top: 1px solid #fe7272;
}

#nav ul {
	margin: 0px;
}

#nav li {
	float: left;
	list-style-type: none;
	font-family: Overpass;
	font-weight: normal;
	font-size: 11pt;
}

#nav li a {
	text-decoration: none;
}

#nav li a:hover {
	color: black;
}

/* Login stuff */

#login,
#logout {
	width: 50px;
	background: url(/app/images/bg_login_button.png) repeat-x red;
	padding: 6px 30px;
	position: absolute;
	right: 10px;
}

#loginbox {
 	background: url(/app/images/bg_login_button.png) repeat-x red;
        padding: 16px;
}

#loginbox ul {
	padding-bottom: 0px;
	margin-bottom: 0px;
	margin-left: 40px;
}

#loginbox p images {
	float: left;
	margin-bottom: 8px;
	margin-top: -5px;
}

#loginbox ul {
	list-style-type: none;
	padding: 0px;
	margin-left: 10px;
}

#loginbox li  {
	background: url(/app/images/bullet_triangle_yellow.png) no-repeat 0px 5px;
	padding-left: 14px;
}

#fancybox-wrap {
  padding: 0px !important;
  top: 0px !important;
  margin-left: 280px;
}

#fancybox-close {
  top: 5px !important;
  right: 5px !important;
}

#fancybox-frame html,
#fancybox-frame body {
	height: 450px !important;
	background: red;
}

#fancybox-outer,
#fancybox-content,
#fancybox-wrap {
	width: 400px !important;
	background: red;
}

/* Page block styling */

#logo {
	margin-top: 1em;
	margin-bottom: 1em;
}


#mainslug {
	margin: 80px 0 .5em;
}

#login a,
#logout a {
	display: block;
	margin-left: auto;
	margin-right: auto;
	text-align: center;
	text-decoration: none;
	text-transform: uppercase;
	font-weight: bold;
}

#head > div.omega {
	padding-right: 10px;
}

#page #pageheader h2,
#page #pageheader p { color: white; }

#pageheader.express { background: url(/app/images/bg_express.png) repeat-x #6699cc; }
#pageheader.flex { background: url(/app/images/bg_flex.png) repeat-x #c88fd7; }
#pageheader.power { background: url(/app/images/bg_power.png) repeat-x #8dd35f; }

#news_title {
        background: #333;
        height: 30px;
}

#news {
        padding-top: 5px;
        padding-bottom: 5px;
        max-height: 20px;
}

#breadcrumb {
	margin-bottom: 1em;
	color: #aaa;
}

#breadcrumb a {
	text-decoration: none;
	color: #aaa !important;
}

#page {
	background: white url(/app/images/bg_gradient.png) repeat-x;
        color: black;
	padding-top: 0px;
	padding-bottom: 5em;
        padding-top: 2em;
}

#page > div {
}

#sidebar_slugs_container {
	background: #e6e6e6;
}


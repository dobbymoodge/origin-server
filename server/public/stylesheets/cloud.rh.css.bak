/* Slideshow stuffs! */

.slideShow ul, .slideShow li {
	list-style-type: none;
	margin: 0px;
	padding: 0px;
}

.slideShow {
	clear: both;
	position: table;
	display: block;
	height: 250px;
}

	li.slide {
		position: absolute;
		display: list-item;
	}

.newsTicker {
	clear: auto;
	height: 30px;
	font-family: 'Overpass';
        background-color: #444;
}

.newsTicker li, #news_title h4 {
	color: white !important;
}


li.news_item {
        width: 800px !important;
}

.slideShow .navigation {
	position: relative;
	clear: both;
	display: block;
	width: 950px;
	margin-left: auto;
	margin-right: auto;
	z-index: 998 !important;
	padding-left: 10px;
	margin-top: 200px;
}

.slideShow .navigation li {
	display: inline;
}

.slideShow .navigation a {
	margin: 0px 2px 0px 0px;
	padding: 3px 8px;
	display: inline-block;
	text-decoration: none;
	font-size: .8em;
	font-weight: bold;
	background-color: white;
	color: #3c6eb4;
	border: 1px solid #3c6eb4;
	font-family: 'Overpass';
}

.slideShow .navigation a:hover, .slideShow .navigation a.selected {
	background-color: #3c6eb4;
	font-weight: bold;
	color: white !important;
	border: 1px solid white;
}

.simpleSlideShow {
	width: 960px;
}

#slides ul {
	width: 960px;
	margin-left: -5px;
}

li.slide {
	width: 960px !important;
}

.slide-canvas {
	position: relative;
	display: block;
	width: 960px !important;
	height: 250px !important;
	margin-left: auto;
	margin-right: auto;
}

.slide-canvas div {
	display: block;
}

#slides #express {
	background: #6699cc;
}
	#slides #express .slide-canvas {
		background: url(/app/images/slide_express.png) no-repeat;
		opacity: .5;
	}
	
#slides #flex {
	background: #c88fd7;
}
	#slides #flex .slide-canvas {
		background:  url(/app/images/slide_flex.png) repeat-x;
		opacity: .5;
	}

.slides li p {
	font-size: 1.1em;
	font-family: 'Overpass';
}

.slide div a { color: white; }

.slide .content {
	position: absolute;
	display: block;
	width: 960px;
	height: 245px;
	top: 5px;
	background: none;
	color: #fff;
	z-index: 980;
	padding: 0.2em 10px;
	font-family: 'Overpass';
}

#slides .content h2 {
	color: white;
	font-size: 25pt;
	text-transform: none;
	margin-top: 0px;
	font-weight: normal;
	font-family: 'Overpass';
	margin-bottom: 0px;
}

#slides .content p {
	font-size: 13pt;
	width: 550px;
	color: white;
}





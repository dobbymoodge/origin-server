}

/* App Promo Banners */

#app_promos .express {
	background: url(/app/images/express-tile-trans.png) #ccc;
}
  #app_promos .express.hover {
    background-color: #B8CFE6;
  }
  #app_promos .express.hover h4 {
    color: #6699cc;
  }
  #app_promos .express.hover a {
    background-color: #6699cc;
  }

#app_promos .flex {
	background: url(/app/images/flex-tile-trans.png) #ccc;
}
  #app_promos .flex.hover{
    background-color: #DCB8E6;
  }
  #app_promos .flex.hover h4 {
    color: #c88fd7;
  }
  #app_promos .flex.hover a {
    background-color: #c88fd7;
  }

#app_promos .power {
	background: url(/app/images/power-tile-trans.png) #ccc;
}
  #app_promos .power.hover {
     background-color: #C7E6B8;
  }
  #app_promos .power.hover h4{
    color: #87cc66;
  }
  #app_promos .power.hover a {
    background-color: #87cc66;
  }

#app_promos h3,
#app_promos h4,
#app_promos p {
	margin-top: 1em !important;
	margin-left: auto;
	margin-right: auto;
	text-align: center;
        font-family: 'Overpass';
}

#app_promos h3 {
        font-size: 10pt;
}

#app_promos a {
	padding: 5px 10px;
	background: #777;
	color: white !important;
	font-weight: bold;
	text-decoration: none;
	text-transform: uppercase;
}

#app_promos .grid_8 {
        min-height: 170px;
}

/* Registration form */

#registration input,
#login-form input,
#loginbox input, {
	border: 1px solid #aaa;
	-moz-border-radius: 4px;
	font-size: 14pt;
	width: 200px;
}

#login-form input,
#registration input {
	float: left;
	margin: 0 5px;
	clear: right;
}

#login-form input.button,
#registration input.create {
	float: none;
	clear: both;
	width: auto;
	margin: 0 145px;
}


#loginbox a {
	font-family: Overpass;
	font-size: 11pt;
	text-decoration: none;
	margin: 10px auto;
}

#registration form {
	width: 500px;
}

input#user_emailAddress,
input#user_password,
input#user_passwordConfirmation,
input#user_password {
	width: 250px !important;
	float: left;
	clear: right;
	height: 24px;
	margin-bottom: 16px;
	margin-left: 16px;
}

label {
	color: #777;
	font-size: 10pt;
	font-weight: bold;
	width: 140px;
	float: left;
	text-align: right;
	clear: left;
	line-height: 30px;
	margin-bottom: 16px;
}

#recaptcha_area {
	float: left;
	margin-left: 10px;
	clear: right;
}

.recaptcha_input_area label {
	line-height: inherit;
}

form p {
	clear: left;
}


#registration td,
#registration th {
	padding-top: 1.5em;
}

#registration table {
	margin-bottom: 40px;
}

#form_notes {
	background: #f2f2f2;
	padding: 10px;
	-moz-border-radius: 4px;

}

#email_tooltips th {
  visibility: hidden;
}

#email_tooltips th,
#email_tooltips td {
  padding-top: 0px;
  vertical-align: top;
}

/* Footer & Subfooter */

#footer ul,
#subfooter ul {
	margin-top: .5em;
	margin-left: -2.2em;
	list-style-type: none;
}

#subfooter li {
	float: left;
}

/* Typography */


p, ul, ol, li, h1, h2, h3, h4, h5, h6, a { color: white; }

#sidebar_slugs_container p, #sidebar_slugs_container ul, #sidebar_slugs_container ol, #sidebar_slugs_container li, #sidebar_slugs_container h1, #sidebar_slugs_container h2, #sidebar_slugs_container h3, #sidebar_slugs_container h4, #sidebar_slugs_container h5, #sidebar_slugs_container h6, #sidebar_slugs_container a { color: #777; }

#sidebar_slugs_container h1, #page h1, #sidebar_slugs_container h2, #page h2, #sidebar_slugs_container h3, #page h3, #sidebar_slugs_container h4, #page h4, #sidebar_slugs_container h5, #page h5, #sidebar_slugs_container h6, #page h6 {
	font-family: 'Overpass';
}

#page p, #page ul, #page ol, #page li, #page h1, #page h2, #page h3, #page h4, #page h5, #page h6, #page a { color: #777; }

#app_promos p, #app_promos ul, #app_promos ol, #app_promos li, #app_promos h1, #app_promos h2, #app_promos h3, #app_promos h4, #app_promos h5, #app_promos h6, #app_promos a { color: #777; }


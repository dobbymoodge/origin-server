-# Sign in dialog box

#signin.dialog
  %header
    %h1
      Sign in to OpenShift

  = render :partial => 'login/signin_form', :locals => { :register_url => '#' }

  %a.close_button{ :href => "#",
                   :title => "Close this dialog" }
    %img{ :src => "/app/images/close_button.png" }

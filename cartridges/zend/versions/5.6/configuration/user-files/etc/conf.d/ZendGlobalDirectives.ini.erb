
; The installation directory
zend.install_dir=/usr/local/zend

; The directory all configuration files are stored in
zend.conf_dir=/usr/local/zend/etc

; The directory all *.pid files are stored in
zend.pid_dir=/usr/local/zend/var/run

; The directory all temporary files are stored in
zend.temp_dir=/usr/local/zend/tmp

; The directory all log files are stored in
zend.log_dir=/usr/local/zend/var/log

; The directory all log files are stored in
zend.data_dir=/usr/local/zend/var

; UID of httpd process
zend.httpd_uid=<%= ENV['OPENSHIFT_ZEND_UID'] %>

; GID of httpd process
zend.httpd_gid=<%= ENV['OPENSHIFT_ZEND_UID'] %>

; The directory where PHP configuration files are located
zend.ini_scandir=conf.d

; The serial number
zend.serial_number=50UI6G100G18501C474DA8DB952EE43A 

; The user name
zend.user_name=OpenShift-0001

; Zend Server Node ID
zend.node_id=0

